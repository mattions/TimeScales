//**************************************************************************
// All_Tau_Vecs loads the tables used to define the taus for channels


objref taum_
objref tauh_
objref vtau_
objref file_

file_ = new File ()

taum_ = new Vector(16,0)
tauh_ = new Vector(16,0)
vtau_ = new Vector(16,0)


sprint(dirstr, "tau_tables/taum_naf.txt")
file_.ropen (dirstr)
taum_.scanf(file_)
file_.close()

sprint(dirstr, "%s/tau_tables/tauh_naf.txt")
file_.ropen (dirstr)
tauh_.scanf(file_)
file_.close()

sprint(dirstr, "%s/tau_tables/vtau_naf.txt")
file_.ropen (dirstr)
vtau_.scanf(file_)
file_.close()

table_taumnaf_naf(taum_,vtau_)
table_tauhnaf_naf(tauh_,vtau_)


//*************************



objref tauh_nap_
objref vtau_nap_
objref file_nap_

