	//***************************
	
	
	
	
	
	//***************************
	//Build GABA synapses
	// soma GABA
	for i = 0, 15 {		// 16 in soma
			soma gaba[i] = new GABAt(0.5) 	synlist.append(gaba[i])
	}
	
	// Inner dends GABA
	for i = 0, 2  {	// 3 each
			dend1 gaba[i] = new GABAt(0.5)    synlist.append(gaba[i]) 
	}
	
	for i = 0, 2  {
			dend2 gaba[i] = new GABAt(0.5)  synlist.append(gaba[i]) 
	
