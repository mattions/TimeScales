			dend2_2 ampa[i] = new AMPAt(onode)  synlist.append(ampa[i]) 
	}
	
	for i = 0, 1  {
			onode = (i+1)/3
			dend3_1 ampa[i] = new AMPAt(onode)  synlist.append(ampa[i]) 
	}
	
	for i = 0, 1  {
