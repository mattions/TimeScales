	//Build AMPA synapses
	// Inner dends AMPA
	for i = 0, 0  {		// 0 in soma, 1 in each prox dend
			dend1 ampa[i] = new AMPAt(0.5)    synlist.append(ampa[i]) 
	}
	
	for i = 0, 0  {
