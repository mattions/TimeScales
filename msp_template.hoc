			dend3_1[1] nmda[i] = new NMDAt(onode)   synlist.append(nmda[i]) 
	}
	
	for i = 0, 3  {
			onode = (i+1)/5		// 0.2, 0.4, 0.6, 0.8
			dend3_1[2] nmda[i] = new NMDAt(onode)   synlist.append(nmda[i]) 
